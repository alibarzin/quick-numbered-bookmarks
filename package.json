{
  "name": "numbered-bookmarks",
  "displayName": "Numbered Bookmarks",
  "description": "Add and jump to numbered (0-9) bookmarks with keyboard shortcuts.",
  "version": "0.1.0",
  "publisher": "alirezabarzin",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alibarzin/numbered-bookmarks"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:numberedBookmarks.toggle0",
    "onCommand:numberedBookmarks.goto0",
    "onCommand:numberedBookmarks.toggle1",
    "onCommand:numberedBookmarks.goto1",
    "onCommand:numberedBookmarks.toggle2",
    "onCommand:numberedBookmarks.goto2",
    "onCommand:numberedBookmarks.toggle3",
    "onCommand:numberedBookmarks.goto3",
    "onCommand:numberedBookmarks.toggle4",
    "onCommand:numberedBookmarks.goto4",
    "onCommand:numberedBookmarks.toggle5",
    "onCommand:numberedBookmarks.goto5",
    "onCommand:numberedBookmarks.toggle6",
    "onCommand:numberedBookmarks.goto6",
    "onCommand:numberedBookmarks.toggle7",
    "onCommand:numberedBookmarks.goto7",
    "onCommand:numberedBookmarks.toggle8",
    "onCommand:numberedBookmarks.goto8",
    "onCommand:numberedBookmarks.toggle9",
    "onCommand:numberedBookmarks.goto9",
    "onCommand:numberedBookmarks.list"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "numberedBookmarks.toggle0",
        "title": "Add Bookmark 0"
      },
      {
        "command": "numberedBookmarks.goto0",
        "title": "Go To Bookmark 0"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+0",
        "command": "numberedBookmarks.toggle0",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+0",
        "command": "numberedBookmarks.goto0",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+0",
        "command": "numberedBookmarks.toggle0",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+0",
        "command": "numberedBookmarks.goto0",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+1",
        "command": "numberedBookmarks.toggle1",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+1",
        "command": "numberedBookmarks.goto1",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+1",
        "command": "numberedBookmarks.toggle1",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+1",
        "command": "numberedBookmarks.goto1",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+2",
        "command": "numberedBookmarks.toggle2",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+2",
        "command": "numberedBookmarks.goto2",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+2",
        "command": "numberedBookmarks.toggle2",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+2",
        "command": "numberedBookmarks.goto2",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+3",
        "command": "numberedBookmarks.toggle3",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+3",
        "command": "numberedBookmarks.goto3",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+3",
        "command": "numberedBookmarks.toggle3",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+3",
        "command": "numberedBookmarks.goto3",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+4",
        "command": "numberedBookmarks.toggle4",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+4",
        "command": "numberedBookmarks.goto4",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+4",
        "command": "numberedBookmarks.toggle4",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+4",
        "command": "numberedBookmarks.goto4",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+5",
        "command": "numberedBookmarks.toggle5",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+5",
        "command": "numberedBookmarks.goto5",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+5",
        "command": "numberedBookmarks.toggle5",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+5",
        "command": "numberedBookmarks.goto5",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+6",
        "command": "numberedBookmarks.toggle6",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+6",
        "command": "numberedBookmarks.goto6",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+6",
        "command": "numberedBookmarks.toggle6",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+6",
        "command": "numberedBookmarks.goto6",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+7",
        "command": "numberedBookmarks.toggle7",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+7",
        "command": "numberedBookmarks.goto7",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+7",
        "command": "numberedBookmarks.toggle7",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+7",
        "command": "numberedBookmarks.goto7",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+8",
        "command": "numberedBookmarks.toggle8",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+8",
        "command": "numberedBookmarks.goto8",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+8",
        "command": "numberedBookmarks.toggle8",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+8",
        "command": "numberedBookmarks.goto8",
        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+shift+9",
        "command": "numberedBookmarks.toggle9",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+9",
        "command": "numberedBookmarks.goto9",
        "when": "editorTextFocus"
      },
      {
        "key": "super+shift+9",
        "command": "numberedBookmarks.toggle9",
        "when": "editorTextFocus && !editorReadonly",
        "mac": true
      },
      {
        "key": "super+9",
        "command": "numberedBookmarks.goto9",
        "when": "editorTextFocus",
        "mac": true
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.130",
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.5.2",
    "mocha": "^11.7.5",
    "typescript": "^4.9.0"
  }
}