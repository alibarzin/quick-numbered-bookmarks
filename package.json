        "when": "editorTextFocus",
        "mac": true
      },
      {
        "key": "ctrl+alt+9",
        "command": "numberedBookmarks.add9",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+9",
        "command": "numberedBookmarks.goto9",
        "when": "editorTextFocus"
      },
      {
        "key": "super+alt+9",
        "command": "numberedBookmarks.add9",
